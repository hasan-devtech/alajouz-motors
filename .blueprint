models:
    Brand: { name: 'string:127', slug: 'string unique', softdeletes: softDeletes, relationships: { hasMany: 'BrandModel, Car, SellingRequest', morphOne: Image } }
    BrandModel: { brand_id: 'id foreign', name: 'string:127', slug: 'string unique', softdeletes: softDeletes, relationships: { hasMany: 'Car, SellingRequest', morphOne: Image } }
    Color: { name: 'string:127', relationships: { hasMany: 'Car, SellingRequest' } }
    Car: { brand_id: 'id foreign', brand_model_id: 'id foreign', color_id: 'id foreign', year: 'timestamp nullable', distance: 'decimal:8,2', engine: 'integer:false,true', engine_type: 'enum:placeholder', price: 'decimal:8,2', vin: string, type: 'enum:placeholder', status: 'enum:placeholder', softdeletes: softDeletes, relationships: { belongsToMany: Customer, morphMany: Image } }
    Customer: { name: string, phone: 'char:15', password: string, softdeletes: softDeletes, relationships: { hasMany: 'SellingRequest, Rate, Renting, Booking', belongsToMany: Car, morphOne: Image } }
    Rate: { customer_id: 'id foreign', car_id: 'id foreign', renting_id: 'id foreign', rate: 'smallInteger:false,true', comment: text }
    Renting: { customer_id: 'id foreign', car_id: 'id foreign', start_date: 'timestamp nullable', end_date: 'timestamp nullable', total_price: 'decimal:8,2', status: 'enum:placeholder', license: string, softdeletes: softDeletes }
    Booking: { customer_id: 'id foreign', car_id: 'id foreign', amount: 'decimal:8,2', visit_date: 'timestamp nullable', status: 'enum:placeholder', softdeletes: softDeletes }
    SellingRequest: { brand_id: 'id foreign', brand_model_id: 'id foreign', customer_id: 'id foreign', color_id: 'id foreign', year: 'timestamp nullable', distance: 'decimal:8,2', engine: 'integer:false,true', engine_type: 'enum:placeholder', price: 'decimal:8,2', price_after_commission: 'decimal:8,2', vin: string, status: 'enum:placeholder', softdeletes: softDeletes, relationships: { morphMany: Image } }
    Image: { path: string, alt: string, softdeletes: softDeletes, relationships: { morphTo: Imageable } }
    OTP: { code: string, request_code: string, phone: string, type: 'enum:placeholder' }
    Payment: { method: 'enum:placeholder', amount: 'decimal:8,2', payment_date: 'timestamp nullable', status: 'enum:placeholder', softdeletes: softDeletes }
    UserActivity: { user_id: 'id foreign', checked_in: 'timestamp nullable', checked_out: 'timestamp nullable', status: 'enum:placeholder', notes: text }
